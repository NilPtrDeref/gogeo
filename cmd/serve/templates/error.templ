package templates

templ Error(err string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1"/>
			<title>GoGeo</title>
			<link href="css/style.css" rel="stylesheet"/>
		</head>
		<body>
			<p id="geojson-container">{ err }</p>
		</body>
	</html>
}
